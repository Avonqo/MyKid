<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    tools:context="za.co.codetribe.mynfirst.HomeActivity">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="140dp"
        android:background="@drawable/factfile"
        android:id="@+id/imageView3"
        android:layout_alignParentStart="true" />
    <ImageView
        android:id="@+id/imageHome"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/home"
        android:elevation="10dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/imageProfile"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/profile"
        android:elevation="10dp"
        android:layout_marginRight="25dp"
        android:layout_marginEnd="43dp"
        android:onClick="edit"
        android:layout_below="@+id/imageView3"
        android:layout_toStartOf="@+id/imageEvents" />

    <ImageView
        android:id="@+id/imageEvents"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/events"
        android:layout_marginRight="65dp"
        android:layout_marginEnd="29dp"
        android:elevation="10dp"
        android:layout_below="@+id/imageView3"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:id="@+id/imageGallery"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/gallery"
        android:layout_below="@+id/imageProfile"
        android:layout_alignLeft="@+id/imageProfile"
        android:layout_alignStart="@+id/imageProfile"
        android:elevation="10dp"/>

    <ImageView
        android:id="@+id/imageActivities"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/activities"
        android:elevation="10dp"
        android:layout_below="@+id/imageEvents"
        android:layout_toEndOf="@+id/imageProfile" />

    <ImageView
        android:id="@+id/imageNotification"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/notification"
        android:elevation="10dp"
        android:layout_below="@+id/imageGallery"
        android:layout_alignLeft="@+id/imageGallery"
        android:layout_alignStart="@+id/imageGallery" />

    <ImageView
        android:id="@+id/imageAboutUs"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/aboutus"
        android:elevation="10dp"
        android:layout_below="@+id/imageActivities"
        android:layout_alignLeft="@+id/imageActivities"
        android:layout_alignStart="@+id/imageActivities" />

    <ImageView
        android:id="@+id/imageInquiries"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@drawable/inquiries"
        android:elevation="10dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="59dp"
        android:layout_below="@+id/imageGallery"
        android:layout_alignStart="@+id/imageHome" />





    </RelativeLayout>
